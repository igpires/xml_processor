<!-- app/views/documents/_form.html.erb -->
<%= form_with model: document, local: true, html: { multipart: true } do |form| %>
  <div class="field">
    <%= form.label :xml_file, "Upload de Arquivo XML" %>
    <%= form.file_field :xml_file %>
  </div>

  <div class="actions">
    <%= form.submit "Enviar XML" %>
  </div>
<% end %>
